{
  "name": "@lastic/uniquery",
  "version": "0.1.0",
  "description": "A sdk for interacting with the coretime chain",
  "repository": "lastic_xyz/lasticUI",
  "license": "GNUv3",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs ./src",
    "lint:fix": "eslint --ext .ts,.js,.mjs,.cjs ./src --fix",
    "prepack": "unbuild",
    "release": "pnpm test && changelogen && git push --follow-tags && npm publish",
    "test": "vitest run --dir test"
  },
  "dependencies": {
    "gql-query-builder": "^3.8.0",
    "ofetch": "^1.3.3",
    "scule": "^1.0.0",
    "ufo": "^1.3.0",
    "@polkadot/api": "^10.6.1",
    "@polkadot/extension-dapp": "^0.46.2",
    "@polkadot/keyring": "^12.1.2",
    "@polkadot/networks": "^12.1.2",
    "@polkadot/types": "^10.6.1",
    "@polkadot/ui-keyring": "^3.3.1",
    "@polkadot/ui-settings": "^3.3.1",
    "@polkadot/util": "^12.1.2",
    "@polkadot/util-crypto": "^12.1.2"
  },
  "devDependencies": {
    "c8": "latest",
    "changelogen": "^0.5.5",
    "eslint": "latest",
    "eslint-config-unjs": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vitest": "latest"
  }
}